{
  "buildCommand": "cd apex-ai && npm run build",
  "outputDirectory": "apex-ai/dist",
  "installCommand": "cd apex-ai && npm install",
  "framework": "vite",
  "functions": {
    "api/chat.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
